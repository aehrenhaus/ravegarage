	<configSections>
    <section name="RBTConfiguration" type="Medidata.RBT.ConfigurationHandlers.RBTConfiguration, Medidata.RBT.ConfigurationHandlers" />
    <section name="RaveConfigurationGroup" type="Medidata.RBT.ConfigurationHandlers.RaveConfigurationGroup, Medidata.RBT.ConfigurationHandlers" />
