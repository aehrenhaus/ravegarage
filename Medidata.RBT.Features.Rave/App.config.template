		TestResultPath="Output"	
		TakeScreenShotsEveryStep ="False" 
		SqlScriptsPath="C:\"
		BrowserName="firefox"
		BrowserPath="..\..\..\Firefox\firefox.exe"
		WebDriverPath="..\..\..\ExternalDlls\WebDrivers\"
		SnapshotName="UAT_Snap"
		DatabaseConnection="RaveConnection"
		GenerateReportAfterTest="true"
		ScenarioNamePrefix="PB_" 
		AutoSaveMimeTypes="application/zip;application/pdf;application/octet-stream;text/xml"
		POAssembly="Medidata.RBT.PageObjects.Rave, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"
		UploadTimeout="400"
		DownloadTimeout="400" 
                EnableSeeding ="True"
                SeleniumServerUrl=""
                RaveConfigurationName="default"
                UploadPath="..\..\..\Uploads"
                DownloadPath="..\..\..\Downloads">
         </RBTConfiguration>

  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <namespace name="Medidata.RBT"/>
